* {
    font: "Open Sans", "Helvetica Neue", Helvetica, Arial, sans-serif;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    -webkit-tap-highlight-color: transparent;
    -webkit-font-smoothing: antialiased;
}

body,
html,
#stake-map {
    width: 100%;
    height: 100%;
    overflow: hidden;
    margin: 0;
}

input {
    border: 0;
}

ul, li {
    list-style: none;
}

#webapp {
    width: 100%;
    height: 100%;
}

#ctp-panel {
    width: inherit;
    height: inherit;
    position: relative;
}

#stake-map-search {
    width: 100%;
    height: 40px;
    line-height: 40px;
    padding: 5px 0;
    background: #fff;
    border-bottom: 1px solid #e5e5e5;
}

.stake-map-scan {
    width: 55px;
    height: 55px;
    background: url("../images/c_main_scan_normal.png") center center no-repeat;
    background-size: 55px 55px;
    position: absolute;
    bottom: 6px;
    right: 60px;
}

.stake-map-scan.clicked {
    background: url("../images/c_main_scan_pressed.png") center center no-repeat;
    background-size: 48px 48px;
}

#stake-map {
    height: 520px;
}

.stake-map-search-left {
    width: 60px;
    height: inherit;
    text-align: center;
    float: left;
    background: url('../images/c_main_more_icon.png') center center no-repeat;
    background-size: 24px 24px;
}

.stake-map-search-center {
    height: 35px;
    overflow: hidden;
    float: left;
    border-radius: 4px;
    border: 1px solid #e5e5e5;
    background: #e5e5e5;
    position: relative;
}

.stake-map-search-center input {
    width: 85%;
    height: 35px;
    border: 0;
    outline: none;
    background: #e5e5e5;
    font-size: 17px;
    line-height: normal; /* for non-ie */
    vertical-align: middle;
    float: left;
    line-height: 35px;
}

.stake-map-search-center span {
    float: left;
    display: inline-block;
    height: 33px;
    width: 13%;
    text-align: center;
    background: url('../images/c_web_search_white_icon.png') 5px center no-repeat;
    background-size: 20px 20px;
}

.stake-map-search-right, .stake-map-search-right-btn {
    width: 50px;
    float: right;
    height: inherit;
    text-align: left;
    color: rgb(47, 185, 195);
    font-size: 16px;
}

.stake-map-search-right-btn {
    display: none;
}

#stack-map {
    z-index: 1;
}

#stack-san {
    position: absolute;
    right: 10px;
    bottom: 10px;
    width: 80px;
    height: 80px;
    border-radius: 40px;
    background: yellow;
    z-index: 2;
    overflow: hidden;
    text-align: center;
    line-height: 80px;
}

.searchTab {
    width: 100%;
    height: 40px;
    padding: 2px 0;
    border-bottom: 1px solid #e5e5e5;
    background: #fdfdfd;
}

.searchTab li {
    height: 40px;
    display: inline-block;
    width: 32%;
    line-height: 30px;
    text-align: center;
}

.searchTab li:nth-child(2) {
    border: 1px solid #e5e5e5;
    border-width: 0 1px;
}

.searchTab li span {
    display: inline-block;
    width: 15px;
    height: 14px;
    margin: 15px 0 11px 0;
    float: left;
    background: url('../images/c_downward_icon.png') left 1px no-repeat;
    background-size: 9px 9px;
}

.searchTab li i {
    font-style: normal;
    display: inline-block;
    height: 40px;
    line-height: 40px;
    float: left;
    text-align: right;
    margin-left: 25%;
    font-size: 15px;
    color: #999;
}

.searchTab li.selected i {
    color: rgb(47, 185, 195);
}

.searchTab li.selected span {
    background: url('../images/c_upward_icon_select.png') left 1px no-repeat;
    background-size: 9px 9px;
}

.searchTab-sub {
    width: 100%;
    position: absolute;
    left: 0;
    top: 82px;
    overflow: hidden;
    display: none;
}

.searchTab-sub ul {
    background: #fff;
    width: 100%;
    float: left;
}

.searchTab-sub ul li {
    height: 30px;
    line-height: 30px;
    border-bottom: 1px solid #e5e5e5;
    text-indent: 25px;
}

.sreen-mask, .ctp-pay-pwd-mask {
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background: #000;
    opacity: .8;
    z-index: 0;
    display: none;
    position: fixed;
}

.ctp-list-box {
    width: 100%;
    overflow: hidden;
    overflow-y: auto;
    background: #f1f5f8;
}

.stake-list dl {
    height: 65px;
    border-bottom: 1px solid #e5e5e5;
    background: #fff;
    overflow: hidden;
    padding: 2px 0;
}

.stake-list dl dt {
    padding: 0 10px;
    height: 30px;
    margin: 0 auto;
    line-height: 40px;
    font-size: 17px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
}

.stake-list dl dt span {
    float: right;
    color: #999;
    line-height: 32px;
    font-size: 12px;
    text-align: right;
}

.stake-list dl dt em {
    padding-left: 25px;
    background: url("../images/c_list_leisure_icon.png") 8px center no-repeat;
    background-size: 12px 12px;
    font-size: 12px;
    color: #999;
    float: left;
    text-indent: 0;
}

.stake-list dl dt i {
    width: 55%;
    font-style: normal;
    float: left;
    text-indent: 0;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.stake-list dl dt em.none {
    background: url("../images/c_list_leisure_icon_gray.png") center center no-repeat;
    background-size: 10px 10px;
}

.stake-list dl dd.location {
    width: 80%;
    float: left;
    color: #999;
    height: 30px;
    line-height: 30px;
    font-size: 13px;
    text-indent: 10px;
}

dd.orderBtn {
    width: 55px;
    padding-top: 0;
    height: 25px;
    float: right;
}

dd.orderBtn span {
    /*display: block;*/
    /*float: right;*/
    /*margin-right: 5px;*/
    /*width: 50px;*/
    /*height: inherit;*/
    /*border-radius: 3px;*/
    /*background: #ccc;*/
    /*color: #fff;*/
    /*text-align: center;*/

    float: right;
    width: 45px;
    height: 25px;
    margin: 0 10px 0 0;
    display: inline-block;
    text-align: center;
    line-height: 25px;
    background: #ccc;
    color: #fff;
    border-radius: 3px;
    font-size: 14px;

}

dd.orderBtn.yes span {
    background: rgb(47, 185, 195);
}

.stake-details-top {
    position: relative;
    height: 140px;
    width: 100%;
    background: #1e1b22;
    /*overflow: hidden;*/
    border-bottom: 1px solid #ccc;
}

.stake-details-icon {
    width: 90px;
    height: 90px;
    padding: 2px;
    border-radius: 3px;
    background: #fff;
    position: absolute;
    left: 10px;
    bottom: 16px;
    z-index: 2;
}

.stake-details-icon img {
    width: 90px;
    height: 90px;
}

.stake-details-top-bottom {
    bottom: 40px;
    position: absolute;
    left: 0;
    height: 40px;
    width: 100%;
    background: #fff;
    z-index: 1;
    text-indent: 95px;
    line-height: 17px;
    color: #666;
    font-size: 11px;
}

.work-time-container {
}

.stake-details-tab,
.receiptOrders-tab {
    width: 100%;
    height: 50px;
    line-height: 50px;
    text-align: center;
    background: #fff;
    left: 0;
    bottom: 0;
    position: absolute;
    z-index: 0;
}

.stake-details-tab {
    height: 40px;
    line-height: 40px;
}

.receiptOrders-tab {
    top: 0;
}

.stake-details-tab li,
.receiptOrders-tab li {
    width: 50%;
    display: inline-block;
    float: left;
    height: inherit;
    border-bottom: 1px solid #ccc;
}

.stake-details-tab li {
    color: #777;
    font-size: 17px;
}

.stake-details-tab li.selected,
.receiptOrders-tab li.selected {
    border-bottom: 1px solid rgb(47, 185, 195);
    color: rgb(47, 185, 195);
}

.stake-details-params {
    position: absolute;
    z-index: 0;
    left: 95px;
    bottom: 90px;
    height: 20px;
    line-height: 20px;
    color: #fff;
    width: 300px;
}

.stake-details-params .star {
    color: #fff;
}

.fav-star {
    width: 30px;
    height: 30px;
    position: absolute;
    right: 5px;
    bottom: 45px;
    color: orange;
    z-index: 2;
    font-size: 21px;
    text-align: center;
    line-height: 40px;
    background: url("../images/c_details_add_favorite.png") center center no-repeat;
    background-size: 30px 30px;
}

.search-result-list-item-params .fav-star {
    background: url("../images/c_map_add_favorite_normal.png") center center no-repeat;
    background-size: 30px 30px;
}

.search-result-list-item-params .faved {
    background: url("../images/c_details_favorite_select.png") center center no-repeat;
    background-size: 30px 30px;
}

.faved {
    background: url("../images/c_details_favorite_select.png") center center no-repeat;
    background-size: 30px 30px;
}

.stake-details-main-order-btn,
.cancel-order {
    width: 100%;
    background: lightseagreen;
    color: #fff;
    height: 45px;
    line-height: 45px;
    text-align: center;
    position: fixed;
    left: 0;
    bottom: 0;
    font-size: 17px;
    display: none;
}

.stake-details-main-order-btn {
    display: block;
}

.stake-details-main-order-btn.no {
    width: 100%;
    background: #ccc;
    color: #fff;
    height: 45px;
    line-height: 45px;
    text-align: center;
    position: fixed;
    left: 0;
    bottom: 0;
    font-size: 17px;
}

.stake-details-main {
    width: 100%;
    /*height: 300px;*/
    background: #f1f5f8;
}

.stake-details-tab-sub-details-location {
    background: #fff;
    border: 1px solid #e5e5e5;
    border-width: 1px 0;
    height: 80px;
    overflow: hidden;
}

.stake-details-tab-sub-details-location .location {
    width: 100%;
    height: 50px;
    line-height: 25px;
    float: left;
    padding-right: 20%;
    border-bottom: 1px solid #e5e5e5;
    font-size: 12px;

    display: table;
}

.stake-details-tab-sub-details-location .location span {
    float: right;
    margin-right: 10px;
    line-height: 80px;
    display: block;
    height: inherit;
    text-align: center;
    font-size: 10px;
    color: #ccc;
    text-indent: 0;
    background: url("../images/c_details_navi_btn_pressed.png") center 5px no-repeat;
    background-size: 38px 24px;
}

.stake-details-tab-sub-details-location .location div {
    font-style: normal;
    padding-left: 10px;

    display: table-cell;
    vertical-align: middle;
}

.stake-details-tab-sub-details-location .icon {
    width: 100%;
    line-height: 30px;
    height: 30px;
    float: left;
    text-indent: 30px;
}

.stake-details-tab-sub-details-location .selected {
    background: url("../images/c_list_parking_icon.png") 10px center no-repeat;
    background-size: 15px 15px;
}

.stake-details-tab-sub-details-carType {
    background: #fff;
    border: 1px solid #e5e5e5;
    border-width: 1px 0;
    height: 67px;
    margin-top: 20px;
}

.stake-details-tab-sub-details-carType span {
    float: right;
    width: 20%;
    line-height: 13px;
    display: block;
    height: 40px;
    padding: 18px 0;
    text-align: center;
    font-size: 12px;
    color: rgb(47, 185, 195);
}

.stake-details-tab-sub-details-carType-list {
    width: 80%;
    height: 57px;
    float: left;
    padding: 5px 0;
    overflow: hidden;
}

.stake-details-tab-sub-details-carType-list li {
    width: 55px;
    height: 55px;
    border: 1px solid #e5e5e5;
    display: inline-block;
    margin-left: 5px;
    text-align: center;
    margin-bottom: 10px;
}

.stake-details-tab-sub-details-carType-list li img {
    width: 33px;
    height: 33px;
    margin: 0 auto;
    border: 0;
}

.stake-details-tab-sub-details-carType-list li span {
    width: 100%;
    display: block;
    text-align: center;
    height: 15px;
    line-height: 15px;
    padding: 0;
    color: #333;
    font-size: 11px;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.stake-details-tab-sub-details-carType-list li:first-child {
    margin-left: 10px;
}

.stake-details-tab-sub-details-list {
    background: #fff;
    border: 1px solid #e5e5e5;
    border-width: 1px 0;
    margin-top: 20px;
}

.stake-details-tab-sub-details-list li {
    height: 100px;
    border-bottom: 1px solid #e5e5e5;
}

.stake-details-tab-sub-details-list li:last-child {
    border: 0;
}

.stake-details-tab-sub-details-list li span.line {
    width: 18%;
    height: 80px;
    margin: 10px 0;
    display: block;
    float: right;
    line-height: 110px;
    font-size: 12px;
    text-align: center;
    color: #999;
    border-left: 1px solid #e5e5e5;
}

/*空闲 */

.stake-details-tab-sub-details-list li span.status-1 {
    background: url("../images/c_filtrate_list_leisure_icon.png") no-repeat center 10px;
    background-size: 35px 35px;
}

/*充电中 被占用 */

.stake-details-tab-sub-details-list li span.status-2 {
    background: url("../images/c_list_not_available_icon@2x.png") no-repeat center 10px;
    background-size: 35px 35px;
}

/*告警 */

.stake-details-tab-sub-details-list li span.status-3 {
    background: url("../images/c_list_alert_icon.png") no-repeat center 10px;
    background-size: 35px 35px;
}

/*离线 */

.stake-details-tab-sub-details-list li span.status-4 {
    background: url("../images/c_list_off_line_icon.png") no-repeat center 10px;
    background-size: 35px 35px;
}

.stake-details-tab-sub-details-list-item {
